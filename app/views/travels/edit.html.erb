
<%= simple_form_for(@travel) do |f| %>
  <%= f.input :address %>
  <%= f.input :destination %>
  <%= f.input :seats_available %>
  <%= f.input :departure_date, as: :date, html5: true %>
  <%= f.input :name_of_vehicle %>
  <%= f.input :price %>
  <%= f.input :photo, as: :file %>
  <%= f.submit %>

<% end %>

